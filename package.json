{
  "name": "write-stone-recorder-app",
  "description": "Write Stone Desktop App",
  "version": "1.0.0",
  "author": "Pawan",
  "license": "BSD",
  "keywords": [
    "Electron",
    "DesktopCapture",
    "WebRTC",
    "Angular 5",
    "Angular CLI",
    "Angular Material 2"
  ],
  "scripts": {
    "prepare:body": "node processBody.js",
    "prepare:electron": "node prepare.js",
    "build-mac": "node --max-old-space-size=4096 node_modules/electron-packager ./src WriteStoneRecorderApp  --platform=darwin --arch=x64 --version=0.36.0 --icon=dist/assets/Icon.icns",
    "build-win32": "node --max-old-space-size=4096 node_modules/electron-packager ./dist WriteStoneRecorderApp --overwrite --asar=true --platform=win32 --arch=ia32 --icon=dist/assets/Icon.icns --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Recorder App\"",
    "build-linux": "node --max-old-space-size=4096 node_modules/electron-packager ./dist WriteStoneRecorderApp --overwrite --asar=true --platform=linux --arch=x64 --icon=dist/assets/Icon.icns --prune=true --out=release-builds",
    "build:dev": "node --max-old-space-size=4096 node_modules/@angular/cli/bin/ng build --app 0 --output-path=dist --prod --env=dev --aot --build-optimizer --vendor-chunk --named-chunks --base-href=. --deploy-url=serve://dist && npm run prepare:body dist development && npm run prepare:electron",
    "build:uat": "node --max-old-space-size=4096 node_modules/@angular/cli/bin/ng build --app 0 --output-path=dist --prod --env=uat --aot --build-optimizer --vendor-chunk --named-chunks --base-href=. --deploy-url=serve://dist && npm run prepare:body dist uat && npm run prepare:electron",
    "build:prod": "node --max-old-space-size=4096 node_modules/@angular/cli/bin/ng build --app 0 --output-path=dist --prod --aot --build-optimizer --vendor-chunk --base-href=. --deploy-url=serve://dist && npm run prepare:body dist production && npm run prepare:electron",
    "start": "electron .",
    "all": "npm run build:prod && npm run build-win32 && npm run start"
  },
  "main": "dist/electron.js",
  "bin": {
    "write-stone-recorder-app": "./launch.js"
  },
  "dependencies": {
    "@angular/animations": "^5.1.2",
    "@angular/common": "^5.1.2",
    "@angular/compiler": "^5.1.2",
    "@angular/core": "^5.1.2",
    "@angular/flex-layout": "^2.0.0-beta.12",
    "@angular/forms": "^5.1.2",
    "@angular/http": "^5.1.2",
    "@angular/platform-browser": "^5.1.2",
    "@angular/platform-browser-dynamic": "^5.1.2",
    "@angular/platform-server": "^5.1.2",
    "@angular/router": "^5.1.2",
    "@angular/service-worker": "^5.1.2",
    "@angular/cdk": "^5.0.2",
    "@angular/material": "^5.0.2",
    "@types/stacktrace-js": "~0.0.32",
    "@types/modernizr": "3.3.0",
    "angular-2-local-storage": "^1.0.1",
    "electron": "~1.7.10",
    "electron-winstaller": "^2.6.3",
    "electron-installer-dmg": "^0.2.1",
    "ffmpeg-static": "^2.1.0",
    "moment": "2.18.1",
    "rxjs": "^5.5.6",
    "http-status-codes": "^1.3.0",
    "zone.js": "^0.8.18",
    "domify": "^1.4.0"
  },
  "devDependencies": {
    "electron-packager": "~10.1.0",
    "@angular/cli": "~1.6.2",
    "@angular/compiler-cli": "^5.1.2",
    "@angular/language-service": "^5.1.2",
    "@angular/platform-server": "^5.1.2",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "^2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~8.0.47",
    "async": "^2.6.0",
    "fs-extra": "^4.0.2",
    "hammer-timejs": "^1.1.0",
    "hammerjs": "^2.0.8",
    "codelyzer": "^4.0.2",
    "typescript": "~2.5.3",
    "uglify-es": "^3.0.28",
    "tslib": "^1.8.0",
    "tslint": "~5.8.0",
    "nedb": "^1.8.0",
    "request": "^2.83.0",
    "stacktrace-js": "^2.0.0",
    "path": "^0.12.7",
    "cheerio": "^1.0.0-rc.2",
    "clean-css": "^4.1.9",
    "concurrently": "^3.5.1",
    "electron-protocol-serve": "^1.3.0"
  }
}
